{
  "name": "@my-album/file-manager",
  "version": "0.0.0",
  "description": "File manager microservice",
  "author": "eduardogomesf",
  "license": "MIT",
  "keywords": [
    "node",
    "typescript",
    "microservice"
  ],
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "scripts": {
    "test": "jest --silent",
    "test:cov": "jest --coverage",
    "build": "rm -rf dist && npx tsc --project tsconfig-build.json && npx tsc-alias -p tsconfig-build.json",
    "lint": "npx eslint . --ext .ts --fix",
    "dev": "ts-node-dev --files -r tsconfig-paths/register ./src/main/server.ts --respawn --transpile-only --ignore-watch node_modules",
    "start": "npm run prisma && node -r tsconfig-paths/register dist/main/server.js",
    "prisma": "npx prisma migrate deploy && npx prisma generate"
  },
  "devDependencies": {
    "@types/amqplib": "0.10.4",
    "@types/archiver": "^6.0.2",
    "@types/cookie": "0.6.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@types/jsonwebtoken": "9.0.6",
    "@types/luxon": "3.3.7",
    "@types/node": "20.9.0",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.598.0",
    "@aws-sdk/s3-presigned-post": "3.598.0",
    "@aws-sdk/s3-request-presigner": "3.598.0",
    "@prisma/client": "5.7.1",
    "archiver": "^7.0.1",
    "chalk": "4.1.2",
    "cookie": "0.6.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "kafkajs": "2.2.4",
    "luxon": "3.4.4",
    "prisma": "5.7.1",
    "redis": "4.6.13",
    "tsconfig-paths": "4.2.0",
    "uuid": "9.0.1"
  }
}